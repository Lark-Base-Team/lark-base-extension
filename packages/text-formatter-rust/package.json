{
  "name": "text-formatter-rust",
  "version": "1.0.0",
  "scripts": {
    "build-st": "RUSTC_BOOTSTRAP=1 wasm-pack build --target web",
    "build-mt": "RUSTC_BOOTSTRAP=1 wasm-pack build --target web --out-dir pkg-parallel -- --features parallel",
    "build": "npm-run-all --parallel build-st build-mt",
    "prepare": "pnpm build"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
